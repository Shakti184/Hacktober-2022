 #include <iostream>

using namespace std;
struct empolyee
{
    int ID;
    char favchar;
    float salary;
};
    
   
int main(){
    struct empolyee yaar;
    
    
    yaar.ID = 1;
    yaar.favchar='y';
    yaar.salary=12000;
   
    
    cout<<"ID "<<yaar.ID<<endl;
    cout<<"favchar "<<yaar.favchar<<endl;
    cout<<"salary "<<yaar.salary<<endl;
    
    
    yaar.ID = 2;
    yaar.favchar='z';
    yaar.salary=10000;
    
    
    cout<<"ID "<<yaar.ID<<endl;
    cout<<"favchar "<<yaar.favchar<<endl;
    cout<<"salary "<<yaar.salary<<endl;
    
    
    yaar.ID = 3;
