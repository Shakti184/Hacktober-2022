    yaar.salary=20000;
    
    
    
    cout<<"ID "<<yaar.ID<<endl;
    cout<<"favchar "<<yaar.favchar<<endl;
    cout<<"salary "<<yaar.salary<<endl;
    
    
    
    yaar.ID = 4;
