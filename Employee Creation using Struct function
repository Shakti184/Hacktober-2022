    yaar.salary=30000;
    
    
    cout<<"ID "<<yaar.ID<<endl;
    cout<<"favchar "<<yaar.favchar<<endl;
    cout<<"salary "<<yaar.salary<<endl;
    
    
    yaar.ID = 5;
    yaar.favchar='anu';
    yaar.salary=40000;
    
    
    cout<<"ID "<<yaar.ID<<endl;
    cout<<"favchar "<<yaar.favchar<<endl;
    cout<<"salary "<<yaar.salary<<endl;
    
    
    yaar.ID = 6;
    yaar.favchar='y';
    yaar.salary=98000;
    
    
    
    cout<<"ID "<<yaar.ID<<endl;
    cout<<"favchar "<<yaar.favchar<<endl;
    cout<<"salary "<<yaar.salary<<endl;
    
    
    yaar.ID = 7;
